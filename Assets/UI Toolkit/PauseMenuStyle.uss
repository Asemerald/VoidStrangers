.image {
    opacity: 0;
    right: -10%;
    transition-property: all;
    transition-duration: 0s;
    background-image: var(--image);
    --image: url("project://database/Assets/01_Art/Sprites/Menu/spr_menu_burdens_3.png?fileID=2800000&guid=02512fefa6458b5439ac9037f25f6c50&type=3#spr_menu_burdens_3");
    height: auto;
}

.image.GraphicsButton {
    transition-property: opacity;
    transition-duration: 1s;
    --image: url("project://database/Assets/01_Art/Sprites/Menu/spr_menu_graphics_3.png?fileID=2800000&guid=6f67b747d6a2ada46bfb25edda9c2e41&type=3#spr_menu_graphics_3");
