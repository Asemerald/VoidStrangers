:root {
}

.button {
    -unity-font-definition: url("project://database/Assets/Fonts/Alkhemikal.ttf?fileID=12800000&guid=3cd2174e3648f4446b134463e7850713&type=3#Alkhemikal");
    background-color: rgba(0, 0, 0, 0);
    transition-property: all;
    transition-duration: 0s;
    transition-timing-function: ease;
    transition-delay: 0s;
    justify-content: center;
    align-self: flex-end;
    align-items: flex-end;
    color: rgb(127, 127, 127);
    right: auto;
    visibility: visible;
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
    font-size: 100px;
}

.selected {
    color: rgb(255, 255, 255);
}

.page {
    background-color: rgba(255, 255, 255, 0);
    background-repeat: no-repeat no-repeat;
}

.page-1 > #page0 {
    background-image: url("project://database/Assets/01_Art/Sprites/Menu/icons/spr_menu_page_icon_a_8.png?fileID=2800000&guid=752703dbb6fc8634dbcd8c16a0ee4e76&type=3#spr_menu_page_icon_a_8");
}

.page-1 > #page1 {
    -unity-background-image-tint-color: rgba(255, 255, 255, 0);
}

.page-1 > #page2 {
    -unity-background-image-tint-color: rgba(255, 255, 255, 0);
}

.page-2 > #page0 {
